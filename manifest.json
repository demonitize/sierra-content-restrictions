{
  "name": "Sierra Content Restrictions",
  "description": "Chrome Extension to block websites for the Sierra Program",
  "version": "1.0",
  "author": "Demonitized",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
    "permissions": ["storage", "activeTab", "scripting", "background", "identity", "loginState","management","webRequestBlocking","<all_urls>"	],
    "action": {
    "default_popup": "popup.html"
  },
  "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
   "options_page": "options.html"
}
